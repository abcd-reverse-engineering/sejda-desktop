    <!-- Search & Replace -->
    <div id="searchReplaceTextPrompt" class="modal app-modal" style="display: none; z-index: 999 !important; right: initial!important; bottom: initial!important; left: 0; top: 20px; overflow: hidden">
        <div class="modal-dialog" style="margin: 30px;">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    <div class="modal-title"><lift:locS>Find & Replace</lift:locS></div>
                </div>
                <div class="modal-body">
                    <form class="form-horizontal" style="margin-bottom: -30px;">
                        <div class="row form-group" style="margin-bottom: 5px;">
                            <div class="col-xs-6">
                                <input type="text" class="form-control input-xs lift:locPlaceholder" placeholder="Find" name="findText" id="findText" value=""/>
                            </div>
                            <div class="col-xs-6">
                                <input type="text" class="form-control input-xs lift:locPlaceholder" placeholder="Replace" name="replaceText" id="replaceText" value=""/>
                            </div>
                        </div>
                        <div class="row form-group search-replace-options">
                            <div class="checkbox">
                                <label>
                                    <input type="checkbox" id="replaceMatchCase" name="replaceMatchCase" style="margin-top: 5px;"> <lift:locS>Match case</lift:locS>
                                </label>
                            </div>
                            <div class="checkbox">
                                <label>
                                    <input type="checkbox" id="replaceMatchLinks" name="replaceMatchLinks" checked="checked" style="margin-top: 5px;"> <lift:locS>Include links</lift:locS>
                                </label>
                            </div>
                        </div>
                    </form>
                </div>
                <div class="modal-footer">
                    <div id="searchStatus" style="position: absolute;bottom: 25px;left: 20px;color: #0b8"></div>
